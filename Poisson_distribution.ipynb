   "source": [
    "import numpy as np\n",
    "import math\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "lam = 1\n",
    "Max = 12\n",
    "Poisson_array = np.zeros(Max)\n",
    "\n",
    "for i in range(Max):\n",
    "    Poisson_array[i] = (lam**i) / math.factorial(i) * np.exp(-lam)\n",
    "\n",
    "plt.bar(range(Max), Poisson_array)\n",
    "plt.show()"
   ]
